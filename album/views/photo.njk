{%extends "base.njk"%}

{%block content%}
  <div class="row">
  {%for name in photos %}
    {{items(name)}}
  {%endfor%}
  </div>
{%endblock%}

{%macro items(name)%}
  <div class="col-xs-6 col-md-3">
    <a href="{{name}}" class="thumbnail">
      <img src="{{name}}" alt="...">
    </a>
  </div>
{%endmacro%}